<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.drivefor2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Drivefor2">
        <activity android:name=".Feedback"></activity>
        <activity android:name=".UserProfile" />
        <activity android:name=".MyVehicle" />
        <activity android:name=".sidebar" />
        <activity android:name=".bottomNavigation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".adddata" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>